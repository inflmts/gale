ln      01;36       # LINK
or      01;38;5;196 # ORPHAN
tw      01;38;5;33  # STICKY_OTHER_WRITABLE
ow      01;38;5;33  # OTHER_WRITABLE
st      01;38;5;33  # STICKY
di      01;38;5;33  # DIR
pi      01;38;5;203 # FIFO
so      01;38;5;208 # SOCK
bd      01;33       # BLK
cd      01;33       # CHR
su      01;32       # SETUID
sg      01;32       # SETGID
ex      01;32       # EXEC
fi      00          # FILE

